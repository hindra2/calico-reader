{
  "expo": {
    "name": "calico-reader",
    "slug": "calico-reader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/Apple/App_store_1024_1x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.calico.reader"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/Android/ic_launcher_xxhdpi.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.calico.reader"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "740af29c-68e5-42b9-b441-b3d79100e3bd"
      }
    }
  }
}
